<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="oauth2_v2_associateDevice">
    <Flows>
        <Flow name="oauth2_v2_associateDevice">
            <Request>
                <Step>
                    <Name>assign_set_profile_v3_version</Name>
                </Step>
            </Request>
        </Flow>
    </Flows>
    <HTTPProxyConnection>
        <BasePath>/oauth2/v2.0/associatedevice</BasePath>
        <VirtualHost>https_vhost</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="associateDevice">
        <TargetEndpoint>associateDevice</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>